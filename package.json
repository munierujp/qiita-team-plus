{
  "name": "qiita-team-plus",
  "private": true,
  "version": "1.4.1",
  "description": "Add useful features to Qiita Team.",
  "scripts": {
    "start": "npm run dev:chrome",
    "build": "npm run build:chrome",
    "build:chrome": "gulp pack --production --vendor=chrome",
    "build:firefox": "gulp pack --production --vendor=firefox",
    "build:opera": "gulp pack --production --vendor=opera",
    "build:edge": "gulp pack --production --vendor=edge",
    "dev": "npm run dev:chrome",
    "dev:chrome": "gulp --watch --vendor=chrome",
    "dev:firefox": "gulp --watch --vendor=firefox",
    "dev:opera": "gulp --watch --vendor=opera",
    "dev:edge": "gulp --watch --vendor=edge",
    "lint": "npm-run-all -p lint:*",
    "lint:js": "eslint --ext .js --ignore-path .gitignore .",
    "lint:css": "stylelint --ignore-path .gitignore '**/*.{css,scss}'",
    "fix": "npm-run-all -p fix:*",
    "fix:js": "eslint --fix --ext .js --ignore-path .gitignore .",
    "fix:css": "stylelint --fix --ignore-path .gitignore '**/*.{css,scss}'",
    "release:major": "gulp release",
    "release:minor": "gulp feature",
    "release:patch": "gulp patch"
  },
  "standard": {
    "globals": [
      "chrome"
    ]
  },
  "devDependencies": {
    "babel-cli": "6.x.x",
    "babel-core": "6.x.x",
    "babel-loader": "7.x.x",
    "babel-preset-env": "1.x.x",
    "chai": "4.x.x",
    "chromereload": "0.x.x",
    "date-fns": "^2.10.0",
    "debounce": "1.x.x",
    "del": "3.x.x",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "gulp": "3.x.x",
    "gulp-bump": "2.x.x",
    "gulp-cache": "0.x.x",
    "gulp-clean-css": "^3.x.x",
    "gulp-filter": "^5.x.x",
    "gulp-git": "^2.x.x",
    "gulp-if": "2.x.x",
    "gulp-imagemin": "3.x.x",
    "gulp-json-transform": "0.x.x",
    "gulp-less": "3.x.x",
    "gulp-livereload": "3.x.x",
    "gulp-plumber": "1.x.x",
    "gulp-sass": "^3.x.x",
    "gulp-sequence": "0.x.x",
    "gulp-sourcemaps": "^2.x.x",
    "gulp-tag-version": "1.x.x",
    "gulp-util": "3.x.x",
    "gulp-zip": "^4.x.x",
    "npm-run-all": "^4.1.5",
    "require-dir": "0.x.x",
    "standard": "^10.0.2",
    "stylelint": "^10.0.1",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^3.6.0",
    "vinyl-named": "1.x.x",
    "vue": "^2.6.10",
    "webpack": "3.x.x",
    "webpack-stream": "3.x.x",
    "yargs": "^8.x.x"
  }
}
