{
  "name": "qiita-team-plus",
  "private": true,
  "version": "1.4.1",
  "description": "Add useful features to Qiita Team.",
  "scripts": {
    "start": "npm run dev:chrome",
    "build": "npm run build:chrome",
    "build:chrome": "gulp pack --production --vendor=chrome",
    "build:firefox": "gulp pack --production --vendor=firefox",
    "build:opera": "gulp pack --production --vendor=opera",
    "build:edge": "gulp pack --production --vendor=edge",
    "dev": "npm run dev:chrome",
    "dev:chrome": "gulp --watch --vendor=chrome",
    "dev:firefox": "gulp --watch --vendor=firefox",
    "dev:opera": "gulp --watch --vendor=opera",
    "dev:edge": "gulp --watch --vendor=edge",
    "lint": "npm-run-all -p lint:*",
    "lint:js": "eslint --ext .js --ignore-path .gitignore .",
    "lint:css": "stylelint --ignore-path .gitignore '**/*.{css,scss}'",
    "fix": "npm-run-all -p fix:*",
    "fix:js": "eslint --fix --ext .js --ignore-path .gitignore .",
    "fix:css": "stylelint --fix --ignore-path .gitignore '**/*.{css,scss}'",
    "release:major": "gulp release",
    "release:minor": "gulp feature",
    "release:patch": "gulp patch"
  },
  "standard": {
    "globals": [
      "chrome"
    ]
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-preset-env": "1.7.0",
    "chai": "4.2.0",
    "chromereload": "0.0.2",
    "date-fns": "2.10.0",
    "debounce": "1.2.0",
    "del": "3.0.0",
    "eslint": "5.16.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-standard": "4.0.0",
    "gulp": "3.9.1",
    "gulp-bump": "2.9.0",
    "gulp-cache": "0.5.0",
    "gulp-clean-css": "3.10.0",
    "gulp-filter": "5.1.0",
    "gulp-git": "2.9.0",
    "gulp-if": "2.0.2",
    "gulp-imagemin": "3.4.0",
    "gulp-json-transform": "0.4.6",
    "gulp-less": "3.5.0",
    "gulp-livereload": "3.8.1",
    "gulp-plumber": "1.2.1",
    "gulp-sass": "3.2.1",
    "gulp-sequence": "0.4.6",
    "gulp-sourcemaps": "2.6.5",
    "gulp-tag-version": "1.3.1",
    "gulp-util": "3.0.8",
    "gulp-zip": "4.2.0",
    "npm-run-all": "4.1.5",
    "require-dir": "0.3.2",
    "standard": "10.0.3",
    "stylelint": "10.0.1",
    "stylelint-config-standard": "18.3.0",
    "stylelint-scss": "3.6.0",
    "vinyl-named": "1.1.0",
    "vue": "2.6.10",
    "webpack": "3.12.0",
    "webpack-stream": "3.2.0",
    "yargs": "8.0.2"
  }
}
